    "cypress": "^14.1.0",
    "cypress-xpath": "^2.0.1",
    "esbuild": "^0.18.3",
    "multiple-cucumber-html-reporter": "^3.9.2",
    "prettier": "2.8.8",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "webpack": "^5.87.0"
  },
  "dependencies": {
    "cucumber-json-report-formatter": "^0.1.4",
    "express": "^5.1.0",
    "fs": "^0.0.1-security",
    "path": "^0.12.7"
  },
  "cypress-cucumber-preprocessor": {
