{
  "scripts": {
    "html-report": "cypress run --spec 'cypress/e2e/*.feature' --browser chrome --headed",
    "test": "cypress run --browser chrome --spec 'cypress/e2e/*.feature' && cucumber-html-reporter --json-dir=cypress/cucumber-json --report-dir=./cypress/cucumber-report --report-title='Cypress Tests' --metadata='{\"App Version\":\"1.0.0\",\"Test Environment\":\"Staging\"}'",
    "test2": "cypress run --browser chrome --spec 'cypress/e2e/*.feature' && ts-node generate-report.ts"
  },
